{
    "name": "silvercommerce/silvercommerce-recipe",
    "type": "silverstripe-recipe",
    "description": "eCommerce system built on the SilverStripe CMS",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-core": "^5",
        "silverstripe/recipe-cms": "^5",
        "silverstripe/silverstripe-omnipay": "^4",
        "i-lateral/silverstripe-searchable": "dev-ss5",
        "dft/silverstripe-users": "^3",
        "silvercommerce/dashboard": "dev-ss5",
        "silvercommerce/catalogue-admin": "dev-ss5",
        "silvercommerce/catalogue-frontend": "dev-ss5",
        "silvercommerce/orders-admin": "dev-ss5",
        "silvercommerce/reports": "dev-ss5",
        "silvercommerce/shoppingcart": "dev-ss5",
        "silvercommerce/discounts": "dev-ss5",
        "silvercommerce/checkout": "dev-ss5",
        "silvercommerce/contact-admin": "dev-ss5",
        "silvercommerce/tax-admin": "dev-ss5",
        "silvercommerce/quantityfield": "dev-ss5",
        "silvercommerce/silvercommerce-theme": "dev-ss5",
        "symbiote/silverstripe-gridfieldextensions": "^4"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silverstripe-searchable"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-dashboard"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-catalogue-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-catalogue-frontend"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-orders-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-reports"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-shoppingcart"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-discounts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-checkout"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-contact-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-tax-admin"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-quantityfield"
        },
        {
            "type": "vcs",
            "url": "https://github.com/bigfork/silvercommerce-theme"
        }
    ],
    "require-dev": {
        "omnipay/dummy": "^3.0"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "project-files": [
            ".gitignore",
            ".htaccess",
            "app/_config/*",
            "app/src/*",
            "index.php",
            "web.config"
        ],
        "public-files": [
            ".htaccess",
            "web.config",
            "index.php"
        ],
        "resources-dir": "_resources"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
