{
    "name": "silvercommerce/silvercommerce-recipe",
    "type": "silverstripe-recipe",
    "description": "SilverStripe recipe for fully featured page and asset content editing",
    "homepage": "http://silverstripe.org",
    "license": "BSD-3-Clause",
    "require": {
        "silverstripe/recipe-cms": "^1.0",
        "silverstripe/userforms": "5.*",
        "silverstripe/silverstripe-omnipay": "dev-master",
        "wilr/silverstripe-googlesitemaps": "dev-master",
        "axllent/silverstripe-scaled-uploads": "^2.1",
        "i-lateral/silverstripe-searchable": "2.x-dev",
        "i-lateral/silverstripe-users": "2.x-dev",
        "silvercommerce/catalogue-admin": "1.*@dev",
        "silvercommerce/catalogue-frontend": "1.*@dev",
        "silvercommerce/tax-admin": "1.*@dev",
        "silvercommerce/orders-admin": "1.*@dev",
        "silvercommerce/contact-admin": "1.*@dev",
        "silvercommerce/shoppingcart": "1.*@dev",
        "silvercommerce/checkout": "1.*@dev",
        "silvercommerce/quantityfield": "1.*@dev",
        "silvercommerce/silvercommerce-theme": "dev-master"
    },
    "require-dev": {
        "phpunit/PHPUnit": "^5.7",
        "omnipay/dummy": "^2.1"
    },
    "config": {
        "process-timeout": 600
    },
    "extra": {
        "project-files": [
            ".htaccess",
            "mysite/*",
            "index.php",
            "web.config"
        ]
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
